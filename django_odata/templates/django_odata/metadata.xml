<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
	<edmx:DataServices>
		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="{{namespace}}">
			{% for entity in entities %}
			{% include 'django_odata/metadata.entity.xml' %}
			{% endfor %}
		</Schema>
		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ODataWebExperimental.{{namespace}}">
			<EntityContainer xmlns:p4="http://schemas.microsoft.com/ado/2009/02/edm/annotation" Name="{{namespace}}Entities" p4:LazyLoadingEnabled="true">
				{% for entset in entitysets %}
				<EntitySet Name="{{entset.name}}" EntityType="{{namespace}}.{{entset.entity.name}}">
				</EntitySet>
				{% endfor %}
			</EntityContainer>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>